	<subDataset name="GruposDetalles" uuid="34081cbc-c665-4716-801c-6a3d9c030506">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="New Data Adapter "/>
		<parameter name="EmpleadoId" class="java.lang.Integer"/>
		<parameter name="ProgramaId" class="java.lang.Integer"/>
		<parameter name="ModalidadId" class="java.lang.Integer"/>
		<parameter name="FechaInicio" class="java.lang.String"/>
		<parameter name="FechaFin" class="java.lang.String"/>
		<parameter name="CicloId" class="java.lang.Integer"/>
		<parameter name="IdiomaCmm" class="java.lang.Integer"/>
		<parameter name="SucursalId" class="java.lang.Integer"/>
		<parameter name="PlantelId" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT "ProgramasGrupos"."PROGRU_Codigo" AS codigo,
	CONCAT( PROG_Codigo,' ',CMM_Valor,' ',PAMOD_Nombre,' Nivel ',FORMAT( PROGRU_Nivel,'00'),' Grupo ',FORMAT( PROGRU_Grupo,'00')) AS nombreGrupo,
	"ProgramasGrupos"."PROGRU_Nivel" AS nivel,
	"PAModalidadesHorarios"."PAMODH_Horario" AS horario,
	"ProgramasGrupos"."PROGRU_SueldoProfesor" AS sueldo
FROM "ProgramasGrupos"
	INNER JOIN "ProgramasIdiomas" ON 
	 "ProgramasIdiomas"."PROGI_ProgramaIdiomaId" = "ProgramasGrupos"."PROGRU_PROGI_ProgramaIdiomaId" 
	INNER JOIN "Programas" ON 
	 "Programas"."PROG_ProgramaId" = "ProgramasIdiomas"."PROGI_PROG_ProgramaId" 
	INNER JOIN "PAModalidades" ON 
	 "PAModalidades"."PAMOD_ModalidadId" = "ProgramasGrupos"."PROGRU_PAMOD_ModalidadId" 
	INNER JOIN "PAModalidadesHorarios" ON 
	 "PAModalidadesHorarios"."PAMODH_PAModalidadHorarioId" = "ProgramasGrupos"."PROGRU_PAMODH_PAModalidadHorarioId" 
	INNER JOIN "Empleados" ON 
	 "Empleados"."EMP_EmpleadoId" = "ProgramasGrupos"."PROGRU_EMP_EmpleadoId" 
	INNER JOIN "SucursalesPlanteles" ON 
	 "SucursalesPlanteles"."SP_SucursalPlantelId" = "ProgramasGrupos"."PROGRU_SP_SucursalPlantelId" 
	INNER JOIN "ControlesMaestrosMultiples" ON 
	 "ControlesMaestrosMultiples"."CMM_ControlId" = "ProgramasIdiomas"."PROGI_CMM_Idioma" 
WHERE 
	 "Empleados"."EMP_EmpleadoId" = $P{EmpleadoId} 
	 AND "ProgramasGrupos"."PROGRU_PROGI_ProgramaIdiomaId" = $P{ProgramaId} 
	 AND "ProgramasGrupos"."PROGRU_FechaInicio" >= $P{FechaInicio} 
	 AND "PAModalidades"."PAMOD_ModalidadId" = $P{ModalidadId} 
	 AND "ProgramasGrupos"."PROGRU_PACIC_CicloId" = $P{CicloId} 
	 AND PROGRU_SP_SucursalPlantelId =  $P{PlantelId} 
	 AND "ControlesMaestrosMultiples"."CMM_ControlId" = $P{IdiomaCmm} 
	 AND "ProgramasGrupos"."PROGRU_SUC_SucursalId" = $P{SucursalId}]]>
		</queryString>
