	<queryString>
		<![CDATA[SELECT
	FORMAT(GETDATE(),'dd/MM/yyyy') AS FechaActual,
	OC_Codigo AS CodigoOC,
	FORMAT(OC_FechaCreacion,'dd/MM/yyyy hh:mm  tt') AS FechaOC,

	PRO_Codigo AS CodigoProveedor,
	PRO_Nombre AS NombreProveedor,
	CONCAT(PRO_Domicilio, ', ',PRO_Colonia, ', ', PRO_Ciudad, ', ', EstadoProveedor.EST_Nombre, ', ', PaisProveedor.PAI_Nombre)  AS DomicilioProveedor,
	PRO_Cp AS CodigoPostalProveedor,
	PRO_Telefono AS TelefonoProveedor,

	SUC_Nombre AS SedeEnvio,
	ALM_Nombre AS AlmacenEnvio,
	COALESCE(DEP_Nombre, '') AS Departamento,
	CASE ALM_MismaDireccionSucursal
		WHEN 1 THEN
			SUC_Domicilio + ', ' + SUC_Colonia + ', ' + SUC_Ciudad + ', ' + COALESCE(EstadoSucursal.EST_Nombre,'-') + ', ' + COALESCE(PaisSucursal.PAI_Nombre,'-')
		ELSE
			ALM_Domicilio + ', ' + ALM_Colonia + ', ' + ALM_Ciudad + ', ' + COALESCE(EstadoAlmacen.EST_Nombre,'-') + ', ' + COALESCE(PaisAlmacen.PAI_Nombre,'-')
	END AS DomicilioEnvio,
	CASE ALM_MismaDireccionSucursal
		WHEN 1 THEN
			SUC_CP
		ELSE
			ALM_CP
	END AS CodigoPostalEnvio,

	OC_TerminoPago AS TerminosPago,
	CONCAT(MON_Nombre,' (',MON_Codigo,')') AS Moneda,
	OC_Comentario AS ComentariosGenerales,
	Creador.USU_Nombre + COALESCE(' ' + Creador.USU_PrimerApellido + COALESCE(' ' + Creador.USU_SegundoApellido,''),'') AS Creador,
	COALESCE(Autorizo.USU_Nombre + COALESCE(' ' + Autorizo.USU_PrimerApellido + COALESCE(' ' + Autorizo.USU_SegundoApellido,''),''),'') AS Autorizo
FROM OrdenesCompra
LEFT JOIN Departamentos ON OC_DEP_DepartamentoId = DEP_DepartamentoId
INNER JOIN Proveedores ON PRO_ProveedorId = OC_PRO_ProveedorId
LEFT JOIN Estados EstadoProveedor ON EstadoProveedor.EST_EstadoId = PRO_EST_EstadoId
LEFT JOIN Paises PaisProveedor ON PaisProveedor.PAI_PaisId = PRO_PAI_PaisId
INNER JOIN Almacenes ON ALM_AlmacenId = OC_ALM_RecepcionArticulosAlmacenId
LEFT JOIN Estados EstadoAlmacen ON EstadoAlmacen.EST_EstadoId = PRO_EST_EstadoId
LEFT JOIN Paises PaisAlmacen ON PaisAlmacen.PAI_PaisId = PRO_PAI_PaisId
INNER JOIN Sucursales ON SUC_SucursalId = ALM_SUC_SucursalId
LEFT JOIN Estados EstadoSucursal ON EstadoSucursal.EST_EstadoId = PRO_EST_EstadoId
LEFT JOIN Paises PaisSucursal ON PaisSucursal.PAI_PaisId = PRO_PAI_PaisId
INNER JOIN Monedas ON MON_MonedaId = OC_MON_MonedaId
INNER JOIN Usuarios Creador ON Creador.USU_UsuarioId = OC_USU_CreadoPorId
LEFT JOIN Usuarios Autorizo ON Autorizo.USU_UsuarioId = OC_USU_AutorizadoPorId
WHERE OC_OrdenCompraId = $P{id}]]>
	</queryString>
