	<subDataset name="GruposProfe" uuid="5fcda59a-86e0-4d7c-b444-6df7e8c716b2">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="New Data Adapter "/>
		<parameter name="P1" class="java.lang.String"/>
		<parameter name="P2" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT "ProgramasGrupos"."PROGRU_Codigo" AS codigoGrupo,
	"PAModalidadesHorarios"."PAMODH_Horario" AS horario,
	"ProgramasGrupos"."PROGRU_Nivel" AS nivel,
	COALESCE( PROGRU_Aula,'No asignada') AS aula,
	COALESCE( PROGRU_Comentarios,'') AS comentarios
FROM "ProgramasGrupos"
	INNER JOIN "Empleados" ON 
	 "Empleados"."EMP_EmpleadoId" = "ProgramasGrupos"."PROGRU_EMP_EmpleadoId" 
	INNER JOIN "PAModalidadesHorarios" ON 
	 "PAModalidadesHorarios"."PAMODH_PAModalidadHorarioId" = "ProgramasGrupos"."PROGRU_PAMODH_PAModalidadHorarioId" 
	 INNER JOIN Sucursales on PROGRU_SUC_SucursalId = SUC_SucursalId
WHERE 
	 "Empleados"."EMP_EmpleadoId" = $P{P1} AND SUC_SucursalId =  $P{P2}
	  AND ($P{FechaInicio} = '' OR "ProgramasGrupos"."PROGRU_FechaInicio" >= $P{FechaInicio})]]>
		</queryString>
		<field name="codigoGrupo" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="codigoGrupo"/>
			<property name="com.jaspersoft.studio.field.label" value="codigoGrupo"/>
		</field>
		<field name="horario" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="horario"/>
			<property name="com.jaspersoft.studio.field.label" value="horario"/>
		</field>
		<field name="nivel" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.name" value="nivel"/>
			<property name="com.jaspersoft.studio.field.label" value="nivel"/>
		</field>
		<field name="aula" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="aula"/>
			<property name="com.jaspersoft.studio.field.label" value="aula"/>
		</field>
		<field name="comentarios" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="comentarios"/>
			<property name="com.jaspersoft.studio.field.label" value="comentarios"/>
		</field>
	</subDataset>
	<subDataset name="Evaluacion" uuid="360f08b1-c449-47ad-a28c-6ebb6f7cf8f0">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="New Data Adapter "/>
		<parameter name="P1" class="java.lang.Integer"/>
		<parameter name="P2" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT actividad,fechaHabil
FROM (
Select
PAAE_Actividad as actividad,
(SELECT DISTINCT fecha
		FROM (
			Select fecha,ROW_NUMBER() OVER (ORDER BY fecha) AS RowNum from dbo.fn_getDiaHabiles(PROGRU_FechaInicio ,PROGRU_FechaFin,PAMOD_Domingo,PAMOD_Lunes,PAMOD_Martes,PAMOD_Miercoles,PAMOD_Jueves,PAMOD_Viernes,PAMOD_Sabado) where fecha is not null
		) as fechaAplicacion where RowNum = PROGIEM_Dias
	) as fechaHabil
from ProgramasIdiomasExamenesDetalles
INNER JOIN ProgramasIdiomasExamenes ON PROGIE_ProgramaIdiomaExamenId = PROGIED_PROGIE_ProgramaIdiomaExamenId
INNER JOIN ProgramasIdiomasNiveles ON PROGIN_ProgramaIdiomaNivelId = PROGIE_PROGIN_ProgramaIdiomaNivelId
INNER JOIN ProgramasIdiomasExamenesModalidades ON PROGIEM_PROGIED_ProgramaIdiomaExamenDetalleId = PROGIED_ProgramaIdiomaExamenDetalleId AND PROGIEM_Dias !='continuo'
INNER JOIN PAActividadesEvaluacion ON PAAE_ActividadEvaluacionId = PROGIED_PAAE_ActividadEvaluacionId
INNER JOIN ControlesMaestrosMultiples ON CMM_ControlId = PROGIED_CMM_TestId
INNER JOIN PAModalidades ON PAMOD_ModalidadId = PROGIEM_PAMOD_ModalidadId
INNER JOIN ProgramasGrupos ON PROGRU_PROGI_ProgramaIdiomaId = PROGIN_PROGI_ProgramaIdiomaId AND PROGIEM_PAMOD_ModalidadId = PROGRU_PAMOD_ModalidadId
WHERE PROGIN_NivelInicial >=1 AND PROGIN_NivelFinal <=3 AND PROGRU_EMP_EmpleadoId = $P{P1}  AND PROGRU_SUC_SucursalId= $P{P2} 
)a1 WHERE a1.fechaHabil is not null ORDER BY actividad]]>
		</queryString>
		<field name="actividad" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="actividad"/>
			<property name="com.jaspersoft.studio.field.label" value="actividad"/>
		</field>
		<field name="fechaHabil" class="java.sql.Date">
			<property name="com.jaspersoft.studio.field.name" value="fechaHabil"/>
			<property name="com.jaspersoft.studio.field.label" value="fechaHabil"/>
		</field>
		<group name="actividad">
			<groupExpression><![CDATA[$F{actividad}]]></groupExpression>
		</group>
		<group name="fechaHabil">
			<groupExpression><![CDATA[$F{fechaHabil}]]></groupExpression>
		</group>
	</subDataset>
