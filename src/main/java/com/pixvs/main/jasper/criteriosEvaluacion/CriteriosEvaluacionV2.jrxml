			Select fecha,ROW_NUMBER() OVER (ORDER BY fecha) AS RowNum from dbo.fn_getDiaHabiles(PROGRU_FechaInicio ,PROGRU_FechaFin,PAMOD_Domingo,PAMOD_Lunes,PAMOD_Martes,PAMOD_Miercoles,PAMOD_Jueves,PAMOD_Viernes,PAMOD_Sabado) where fecha is not null
		) as fechaAplicacion where RowNum = PROGIEM_Dias
	) as fechaHabil
from ProgramasIdiomasExamenesDetalles
INNER JOIN ProgramasIdiomasExamenes ON PROGIE_ProgramaIdiomaExamenId = PROGIED_PROGIE_ProgramaIdiomaExamenId
INNER JOIN ProgramasIdiomasNiveles ON PROGIN_ProgramaIdiomaNivelId = PROGIE_PROGIN_ProgramaIdiomaNivelId
INNER JOIN ProgramasIdiomasExamenesModalidades ON PROGIEM_PROGIED_ProgramaIdiomaExamenDetalleId = PROGIED_ProgramaIdiomaExamenDetalleId AND PROGIEM_Dias !='continuo'
INNER JOIN PAActividadesEvaluacion ON PAAE_ActividadEvaluacionId = PROGIED_PAAE_ActividadEvaluacionId
INNER JOIN ControlesMaestrosMultiples ON CMM_ControlId = PROGIED_CMM_TestId
INNER JOIN PAModalidades ON PAMOD_ModalidadId = PROGIEM_PAMOD_ModalidadId
INNER JOIN ProgramasGrupos ON PROGRU_PROGI_ProgramaIdiomaId = PROGIN_PROGI_ProgramaIdiomaId AND PROGIEM_PAMOD_ModalidadId = PROGRU_PAMOD_ModalidadId
WHERE PROGIN_NivelInicial >=13 AND PROGIN_NivelFinal <=15 AND PROGRU_EMP_EmpleadoId = $P{P1}  AND PROGRU_SUC_SucursalId= $P{P2} 
)a1 WHERE a1.fechaHabil is not null ORDER BY actividad]]>
		</queryString>
