	<queryString>
		<![CDATA[SELECT
	EMP_EmpleadoId,
	EMP_CodigoEmpleado AS noCertificado,
	'499790905' AS noPoliza,
	'11546' AS noContrato,
	CONCAT('CENPRO / ', ENT_Codigo) AS dependencia,
	'' AS codigoCliente,
	CONCAT(EMP_Nombre, ' ', EMP_PrimerApellido, ' ', NULLIF(EMP_SegundoApellido, '')) AS nombreCompleto,
	FORMAT(EMP_FechaNacimiento, 'dd') AS diaNacimiento,
	FORMAT(EMP_FechaNacimiento, 'MM') AS mesNacimiento,
	FORMAT(EMP_FechaNacimiento, 'yyyy') AS anioNacimiento,
	LEFT(genero.CMM_Valor, 1) AS genero,
	'' AS puesto,
	'' AS gradoEstudios,
	-- Fecha Expedicion
	FORMAT(GETDATE(), 'dd') AS diaExpedicion,
	FORMAT(GETDATE(), 'MM') AS mesExpedicion,
	FORMAT(GETDATE(), 'yyyy') AS anioExpedicion,
	-- Certificado
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'dd') AS diaCertInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'MM') AS mesCertInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'yyyy') AS anioCertInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'dd') AS diaCertFin,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'MM') AS mesCertFin,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'yyyy') AS anioCertFin,
	CONCAT(DATEDIFF(DAY, vigencias.CMM_POLIZA_FECHA_INICIO, vigencias.CMM_POLIZA_FECHA_FIN), ' días') AS duracionCertificado,
	-- Poliza
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'dd') AS diaPolInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'MM') AS mesPolInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_INICIO AS DATE), 'yyyy') AS anioPolInicio,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'dd') AS diaPolFin,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'MM') AS mesPolFin,
	FORMAT(CAST(vigencias.CMM_POLIZA_FECHA_FIN AS DATE), 'yyyy') AS anioPolFin,
	CONCAT(DATEDIFF(DAY, vigencias.CMM_POLIZA_FECHA_INICIO, vigencias.CMM_POLIZA_FECHA_FIN), ' días') AS duracionPoliza
FROM Empleados
INNER JOIN Entidades ON EMP_ENT_EntidadId = ENT_EntidadId
INNER JOIN ControlesMaestrosMultiples genero ON EMP_CMM_GeneroId = genero.CMM_ControlId
INNER JOIN(
	SELECT * FROM (
		select
		CMM_Control as ctrl, CMM_Valor AS valor
		from ControlesMaestrosMultiples WHERE CMM_ControlId IN(2000690, 2000691)
	) as datos
	UNPIVOT (value for name in ([valor])) up
	PIVOT (max(value) for ctrl in ([CMM_POLIZA_FECHA_INICIO],[CMM_POLIZA_FECHA_FIN])) AS result
) AS vigencias ON 1 = 1
WHERE EMP_EmpleadoId =  $P{ID_EMPLEADO}]]>
	</queryString>
