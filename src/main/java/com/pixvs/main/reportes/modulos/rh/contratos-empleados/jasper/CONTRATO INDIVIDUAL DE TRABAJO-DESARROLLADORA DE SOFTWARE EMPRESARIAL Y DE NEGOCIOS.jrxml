		<parameter name="idEmpleado" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
		TRIM(CONCAT(TRIM(EMPBE_Nombre), ' ', TRIM(EMPBE_SegundoApellido), ' ', TRIM(NULLIF(EMPBE_SegundoApellido, '')) ) ) AS nombre,
		CMM_Valor AS parentesco,
		CONCAT(EMPBE_Porcentaje, '%') AS porcentaje
	FROM EmpleadosBeneficiarios
	INNER JOIN ControlesMaestrosMultiples ON EMPBE_CMM_ParentescoId = CMM_ControlId
	WHERE EMPBE_EMP_EmpleadoId =  $P{idEmpleado}]]>
