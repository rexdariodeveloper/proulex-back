	<queryString>
		<![CDATA[SELECT ROW_NUMBER() OVER(ORDER BY curso, modalidad, primerApellido, segundoApellido, nombre, fechaInicio, nivel, descuentoUDG) AS id,
       sede,
       cliente,
       primerApellido,
